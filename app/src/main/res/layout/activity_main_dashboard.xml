<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Main_dashboard">

    <TextView
        android:id="@+id/textView3"
        android:layout_width="377dp"
        android:layout_height="138dp"
        android:text="TextView"
        android:textSize="30sp"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="5dp"
        tools:text="How much would you like to drink ?" />

    <Button
        android:id="@+id/slightlybuzzed"
        android:layout_width="135dp"
        android:layout_height="91dp"
        android:layout_marginStart="17dp"
        android:layout_marginLeft="17dp"
        android:layout_marginTop="36dp"
        android:layout_marginEnd="80dp"
        android:layout_marginRight="80dp"
        android:layout_marginBottom="16dp"
        android:text="Slightly buzzed"
        app:layout_constraintBottom_toTopOf="@+id/party"
        app:layout_constraintEnd_toStartOf="@+id/buzzed"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView3" />

    <Button
        android:id="@+id/buzzed"
        android:layout_width="135dp"
        android:layout_height="91dp"
        android:layout_marginStart="80dp"
        android:layout_marginLeft="80dp"
        android:layout_marginTop="37dp"
        android:layout_marginBottom="16dp"
        android:text="Buzzed"
        app:layout_constraintBottom_toTopOf="@+id/college"
        app:layout_constraintStart_toEndOf="@+id/slightlybuzzed"
        app:layout_constraintTop_toBottomOf="@+id/textView3" />

    <Button
        android:id="@+id/party"
        android:layout_width="135dp"
        android:layout_height="91dp"
        android:layout_marginTop="16dp"
        android:text="At a party "
        app:layout_constraintTop_toBottomOf="@+id/slightlybuzzed"
        tools:layout_editor_absoluteX="16dp" />

    <Button
        android:id="@+id/college"
        android:layout_width="135dp"
        android:layout_height="91dp"
        android:layout_marginStart="80dp"
        android:layout_marginLeft="80dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="18dp"
        android:layout_marginRight="18dp"
        android:text="College students on a fiday"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/party"
        app:layout_constraintTop_toBottomOf="@+id/buzzed" />

    <Button
        android:id="@+id/gohome"
        android:layout_width="351dp"
        android:layout_height="88dp"
        android:text="I WANT TO GO HOME"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="417dp" />
</android.support.constraint.ConstraintLayout>