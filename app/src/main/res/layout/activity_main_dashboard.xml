<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Main_dashboard">

    <TextView
        android:id="@+id/textView3"
        android:layout_width="346dp"
        android:layout_height="92dp"
        android:layout_marginStart="19dp"
        android:layout_marginLeft="19dp"
        android:layout_marginTop="23dp"
        android:layout_marginEnd="19dp"
        android:layout_marginRight="19dp"
        android:layout_marginBottom="12dp"
        android:text="How much would you like to drink?"
        android:textSize="30sp"
        app:layout_constraintBottom_toTopOf="@+id/buzzed"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/slightlybuzzed"
        android:layout_width="135dp"
        android:layout_height="91dp"
        android:layout_marginStart="33dp"
        android:layout_marginLeft="33dp"
        android:layout_marginTop="31dp"
        android:text="Slightly buzzed"
        app:layout_constraintBottom_toTopOf="@+id/gohome"
        app:layout_constraintEnd_toStartOf="@+id/college"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/buzzed" />

    <Button
        android:id="@+id/buzzed"
        android:layout_width="135dp"
        android:layout_height="91dp"
        android:layout_marginStart="33dp"
        android:layout_marginLeft="33dp"
        android:layout_marginTop="12dp"
        android:layout_marginBottom="31dp"
        android:text="Buzzed"
        app:layout_constraintBottom_toTopOf="@+id/slightlybuzzed"
        app:layout_constraintEnd_toStartOf="@+id/party"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView3" />

    <Button
        android:id="@+id/party"
        android:layout_width="135dp"
        android:layout_height="91dp"
        android:layout_marginStart="48dp"
        android:layout_marginLeft="48dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="33dp"
        android:layout_marginRight="33dp"
        android:layout_marginBottom="24dp"
        android:text="At a party "
        app:layout_constraintBottom_toTopOf="@+id/college"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/buzzed"
        app:layout_constraintTop_toBottomOf="@+id/textView3" />

    <Button
        android:id="@+id/college"
        android:layout_width="135dp"
        android:layout_height="91dp"
        android:layout_marginStart="48dp"
        android:layout_marginLeft="48dp"
        android:layout_marginTop="24dp"
        android:layout_marginEnd="33dp"
        android:layout_marginRight="33dp"
        android:layout_marginBottom="32dp"
        android:text="College students on a friday"
        app:layout_constraintBottom_toTopOf="@+id/gohome"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/slightlybuzzed"
        app:layout_constraintTop_toBottomOf="@+id/party" />

    <Button
        android:id="@+id/gohome"
        android:layout_width="175dp"
        android:layout_height="88dp"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="44dp"
        android:layout_marginEnd="10dp"
        android:layout_marginRight="10dp"
        android:layout_marginBottom="39dp"
        android:text="I WANT TO GO HOME"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/PANICHELP"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/slightlybuzzed" />

    <Button
        android:id="@+id/PANICHELP"
        android:layout_width="167dp"
        android:layout_height="90dp"
        android:layout_marginTop="38dp"
        android:layout_marginEnd="16dp"
        android:layout_marginRight="16dp"
        android:layout_marginBottom="28dp"
        android:text="PANIC!!!"
        android:textSize="30sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/gohome"
        app:layout_constraintTop_toBottomOf="@+id/college" />
</android.support.constraint.ConstraintLayout>